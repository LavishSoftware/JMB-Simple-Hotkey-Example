{
	"$schema": "http://www.lavishsoft.com/schema/agent.json",
	"name": "Simple Hotkey Example",
	"platforms": {
		"joe multiboxer session": {
			"eventHandlers": {
				"onAgentStartup": {
					"code": [
						"run SimpleHotkeyExample.Session.iss"
					]
				},
				"onAgentShutdown": {
					"code": [
						"endscript SimpleHotkeyExample.Session"
					]
				}
			}
		}		
	}
}